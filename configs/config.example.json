{
  "github_tokens": [
    "YOUR_GITHUB_TOKEN_HERE"
  ],
  "storage": {
    "primary_path": "//192.168.1.66/plex3/codelupe/repos",
    "backup_path": "//192.168.1.66/plex3/codelupe/backup/repos",
    "max_primary_gb": 14000,
    "max_backup_gb": 10000
  },
  "performance": {
    "workers_per_token": 4,
    "repos_per_hour_per_token": 5000,
    "max_requests_per_second": 2,
    "concurrent_clones": 8
